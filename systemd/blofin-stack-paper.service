[Unit]
Description=Blofin confirmation + paper trading engine
After=network-online.target blofin-stack-ingestor.service
Wants=network-online.target

[Service]
Type=simple
WorkingDirectory=/home/rob/.openclaw/workspace/blofin-stack
EnvironmentFile=/home/rob/.openclaw/workspace/blofin-stack/.env
ExecStart=/home/rob/.openclaw/workspace/blofin-stack/.venv/bin/python /home/rob/.openclaw/workspace/blofin-stack/paper_engine.py
Restart=always
RestartSec=3

[Install]
WantedBy=default.target
