[Unit]
Description=Blofin websocket research monitor
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
WorkingDirectory=/home/rob/.openclaw/workspace/blofin-research
EnvironmentFile=/home/rob/.openclaw/workspace/blofin-research/.env
ExecStart=/home/rob/.openclaw/workspace/blofin-research/.venv/bin/python /home/rob/.openclaw/workspace/blofin-research/ws_monitor.py
Restart=always
RestartSec=3

[Install]
WantedBy=default.target
